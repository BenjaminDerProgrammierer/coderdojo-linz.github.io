body{font-family:open sans,helvetica neue,Arial,sans-serif;font-size:16px;position:relative;padding-bottom:120px;min-height:100vh}img{max-width:100%}section{padding-top:60px;padding-bottom:60px}h1,h2,h3,h4,h5,h6{margin-top:0;margin-bottom:0;margin-block-start:3rem;margin-block-end:1rem}h1,h2{text-align:center}h1::after,h2::after{display:block;width:50px;text-align:center;content:'';margin:1rem auto 0;margin-block-end:2rem;border-bottom:3px solid #03a9f4}p{color:#333}p:empty{margin-bottom:0}hr.full{border-color:#03a9f4;border-width:1px;width:100%;margin-block-start:2rem;margin-block-end:2rem}pre{padding:10px 20px}.navbar{height:50px;background-color:#fff!important;font-weight:700;text-transform:uppercase;font-size:14px;box-shadow:2px 2px 15px 7px rgba(0,0,0,.1);border-bottom:1px solid transparent;transition:all .5s}.navbar .dropdown-item{text-transform:none}.navbar .nav-item a.nav-link{color:#333;padding-left:16px;padding-right:16px}.navbar .nav-item.active a.nav-link{color:#03a9f4}.navbar .navbar-toggler{font-size:14px}.navbar .navbar-collapse{background-color:transparent}.navbar.hero-navbar.navbar-scroll-top{background-color:transparent!important;box-shadow:none;border-bottom:1px solid rgba(255,255,255,.3)}.navbar.hero-navbar.navbar-scroll-top .nav-item a.nav-link{color:#fff}footer{text-align:center;background:#272727;padding:2rem 0;position:absolute;width:100%;bottom:0}footer p{font-weight:300;font-size:12px;color:#fff;letter-spacing:2px}footer p a{color:#fff}footer p a:hover{color:#fff;text-decoration:none}ol.breadcrumb{background-color:transparent;padding:0;margin-block-start:2rem;margin-block-end:1rem}nav.breadcrumb-nav+h1{margin-block-start:0}.hero-area{height:100vh;display:flex;justify-content:center;align-items:center;background:linear-gradient(rgba(0,0,0,0.9),rgba(0,0,0,0.3)),url(img/hero-image.png);background-position:50% 100%;background-size:cover;background-attachment:fixed}section.events .event-type{margin-right:10px}section.events .event-type-coderdojo{background-color:#03a9f4}section.events .event-type-coderdojo-virtual{background-color:#8bc34a}section.events .event-type-playground{background-color:#ff8f00}section.events .event-type-classroom{background-color:#9575cd}section.events .workshop{font-size:calc(1rem - 2px)}section.events .workshop h2{margin-top:0;font-size:1rem;font-weight:600}section.mentors h4{margin-block-start:1rem;margin-block-end:0}section.mentors h5{font-size:14px;margin-block-start:.5rem}section.mentors .mentor{text-align:center}section.mentors .img-box{position:relative;width:100%;max-width:200px;margin-left:auto;margin-right:auto}section.mentors .img-box:before{content:"";display:block;padding-top:100%}section.mentors img{position:absolute;top:0;left:0;bottom:0;right:0;width:100%;height:100%;object-fit:cover;border-radius:50%;box-shadow:2px 2px 15px 7px rgba(0,0,0,.05)}